{
    "type": "object",
    "title": "Anchor menu",
    "id": "Anchor nav",
    "properties": {
        "tab_title": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "template": "{{title}}",
            "watch": {
                "title": "widget.___anchor-nav___"
            }
        },
        "___anchor-nav___": {            
            "type": "string",
            "options": {
                "hidden": true
            },            
            "default": "Anchor menu"
        },
        "anchor-nav-preview": {
            "title": "Example:",
            "type": "string",
            "format": "hidden",
            "default": "/frontend/web/js/app/widgets/anchor-nav/img/preview.jpg",
            "links": [
                {
                    "href": "{{self}}",
                    "mediaType": "image"
                }
            ]

        }
    }

}