{
  "type": "object",
  "title": "Social",
  "properties": {
    "tab_title": {
      "type": "string",
      "options": {
        "hidden": true
      },
      "template": "{{title}}",
      "watch": {
        "title": "widget.___social___"
      }
    },
    "___social___": {
      "type": "string",
      "options": {
        "hidden": true
      },
      "default": "Social"
    },
    "social-preview": {
      "title": "Example:",
      "type": "string",
      "format": "hidden",
      "default": "/frontend/web/js/app/widgets/social/img/preview.jpg",
      "links": [
        {
          "href": "{{self}}",
          "mediaType": "image"
        }
      ]
    },
    "title": {
      "type": "string",
      "title": "Title",
      "default": "RENAULT В СОЦИАЛЬНЫХ СЕТЯХ"
    },
    "FbTitle":{
      "type": "string",
      "title": "Facebook Title",
      "default": "Renault Украина"
    },
    "YtTitle":{
      "type": "string",
      "title": "Youtube Title",
      "default": "Renault Украина"
    },
    "load_more_text": {
      "type": "string",
      "title": "Title",
      "default": "ЗАВАНТАЖИТИ БІЛЬШЕ"
    },
    "wordSlice": {
      "type": "number",
      "title": "slice message (number of words)",
      "default": 20
    }
  }
}