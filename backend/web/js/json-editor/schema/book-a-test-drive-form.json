{
    "type": "object",
    "title": "Book a test drive form",
    "properties": {
        "tab_title": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "template": "{{title}}",
            "watch": {
                "title": "widget.___book-a-test-drive-form___"
            }
        },
        "___book-a-test-drive-form___": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "default": "Book a test drive form"
        },
        "book-a-test-drive-form-preview": {
            "title": "Example:",
            "type": "string",
            "format": "hidden",
            "default": "/frontend/web/js/app/widgets/book-a-test-drive-form/img/preview.jpg",
            "links": [
                {
                    "href": "{{self}}",
                    "mediaType": "image"
                }
            ]

        },
        "book_a_test_drive": {
            "type": "string",
            "title": "Book a test drive",
            "default": "ЗАПИСАТИСЯ НА ТЕСТ-ДРАЙВ"
        },
        "experience_renault_for_yourself": {
            "type": "string",
            "title": "Experience Renault for yourself.",
            "default": "Experience Renault for yourself."
        },
        "i_d_like_to_book_a_test_drive": {
            "type": "string",
            "title": "I’d like to book a test drive",
            "default": "Я ХОЧУ ЗАПИСАТИСЯ НА ТЕСТ-ДРАЙВ"
        },
        "please_select": {
            "type": "string",
            "title": "Please select",
            "default": "Оберіть будь ласка"
        },
        "category_title": {
            "type": "string",
            "title": "Category title",
            "default": "Новинки"
        },
        "subcategory_title": {
            "type": "string",
            "title": "Subcategory title",
            "default": "Автомобілі"
        },
        "find_a_dealer": {
            "type": "string",
            "title": "Find a dealer",
            "default": "Знайти дилера"
        },

        "your_contact_info": {
            "type": "string",
            "title": "Your contact info",
            "default": "ВАША КОНТКТНА ІНФОРМАЦІЯ"
        },
        "book_a_test_drive": {
            "type": "string",
            "title": "Book a test drive",
            "default": "Забронювати тест драйв"
        },

        "items": {
            "type": "array",
            "title": "Images for slider.",
            "options": {
                "collapsed": true
            },
            "items": {
                "type": "object",
                "title": "Vehicle images",
                "options": {
                    "collapsed": true
                },
                "properties": {
                    "title": {
                        "type": "string",
                        "title": "Title.",
                        "default": "Dokker"
                    },
                    "img_src": {
                        "type": "string",
                        "format": "url",
                        "title": "Some image.",
                        "options": {
                            "upload": true
                        },
                        "links": [
                            {
                                "href": "{{self}}",
                                "rel": "View file"
                            }
                        ]
                    }
                }
            }
        },
      "t": {
        "type": "array",
        "format": "table",
        "title": "Translations",
        "uniqueItems": true,
        "options": {
          "collapsed": true
        },
        "items": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string",
              "options": {
                "input_width": "300px"
              }
            },
            "value": {
              "type": "string",
              "options": {
                "input_width": "400px"
              }
            }
          }
        }
      }
    }

}