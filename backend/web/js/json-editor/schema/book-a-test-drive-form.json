{
    "type": "object",
    "title": "Book a test drive form",
    "properties": {
        "tab_title": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "template": "{{title}}",
            "watch": {
                "title": "widget.___book-a-test-drive-form___"
            }
        },
        "___book-a-test-drive-form___": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "default": "Book a test drive form"
        },
        "book-a-test-drive-form-preview": {
            "title": "Example:",
            "type": "string",
            "format": "hidden",
            "default": "/frontend/web/js/app/widgets/book-a-test-drive-form/img/preview.jpg",
            "links": [
                {
                    "href": "{{self}}",
                    "mediaType": "image"
                }
            ]

        },
        "title": {
            "type": "string",
            "title": "Title",
            "default": "ЗАПИСАТИСЯ НА ТЕСТ-ДРАЙВ"
        },
                "items": {
            "type": "array",
            "title": "Images for slider.",
            "options": {
                "collapsed": true
            },
            "items": {
                "type": "object",
                "title": "Vehicle images",
                "options": {
                    "collapsed": true
                },
                "properties": {
                    "title": {
                        "type": "string",
                        "title": "Title.",
                        "default": "Dokker"
                    },
                    "img_src": {
                        "type": "string",
                        "format": "url",
                        "title": "Some image.",                        
                        "options": {
                            "upload": true
                        },
                        "links": [
                            {
                                "href": "{{self}}",
                                "rel": "View file"
                            }
                        ]
                    }
                }
            }
        }
    }

}