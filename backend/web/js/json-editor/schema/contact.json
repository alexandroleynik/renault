{
  "type": "object",
  "title": "Contact",
  "properties": {
    "tab_title": {
      "type": "string",
      "options": {
        "hidden": true
      },
      "template": "{{title}}",
      "watch": {
        "title": "widget.___contact___"
      }
    },
    "___contact___": {
      "type": "string",
      "options": {
        "hidden": true
      },
      "default": "contact"
    },
    "contact-preview": {
      "title": "contact:",
      "type": "string",
      "format": "hidden",
      "default": "/frontend/web/js/app/widgets/contact/img/preview.jpg",
      "links": [
        {
          "href": "{{self}}",
          "mediaType": "image"
        }
      ]
    },
    "title": {
      "type": "string",
      "title": "contact",
      "default": "contact"
    },
    "subscribe_email": {
      "type": "string",
      "title": "subscribe_email",
      "default": "subscribe_email"
    },
    "consent": {
      "type": "string",
      "title": "consent",
      "default": "<a href='http://m.renault.ua/uk/privacy-policy' target='_blank'> ДАЮ СВОЮ ЗГОДУ НА ОБРОБКУ ЗАЗНАЧЕНИХ МНОЮ ВИЩЕ ПЕРСОНАЛЬНИХ ДАНИХ</a>"
    },
    "subscribe_to_news": {
      "type": "string",
      "title": "Subscribe_to_news",
      "default": "Підписатися на новини Renault"
    },
    "subscribe_sms": {
      "type": "string",
      "title": "subscribe_sms",
      "default": "subscribe_sms"
    },
    "phone": {
      "type": "string",
      "title": "phone",
      "default": "Мобільний телефон"
    },
    "comment": {
      "type": "string",
      "title": "comment",
      "default": "Ваше питання"
    },
    "patronymic": {
      "type": "string",
      "title": "patronymic",
      "default": "По батькові"
    },
    "surname": {
      "type": "string",
      "title": "surname",
      "default": "Прізвище"
    },

    "E_Mail": {
      "type": "string",
      "title": "E_Mail",
      "default": "E-Mail"
    },
    "name": {
      "type": "string",
      "title": "name",
      "default": "Ім'я"
    },
    "accost": {
      "type": "string",
      "title": "accost",
      "default": "Звертання"
    },
    "Mr": {
      "type": "string",
      "title": "Mr",
      "default": "Пан"
    },
    "Ms": {
      "type": "string",
      "title": "Ms",
      "default": "Пані"
    },
    "Contact_Renault_in_Ukraine": {
      "type": "string",
      "title": "Зв'язатися з Renault в Українi",
      "default": "Зв'язатися з Renault в Українi"
    },
    "Search_button": {
      "type": "string",
      "title": "Search_button",
      "default": "Search_button"
    },
    "contact_info": {
      "type": "string",
      "title": "contact_info",
      "default": "contact_info"
    },
    "send": {
      "type": "string",
      "title": "Відправити",
      "default": "Відправити"
    },
    "t": {
      "type": "array",
      "format": "table",
      "title": "Translations",
      "uniqueItems": true,
      "options": {
        "collapsed": true
      },
      "items": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "options": {
              "input_width": "300px"
            }
          },
          "value": {
            "type": "string",
            "options": {
              "input_width": "400px"
            }
          }
        }
      }
    }
  }
}