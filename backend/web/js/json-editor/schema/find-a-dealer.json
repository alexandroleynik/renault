{
    "type": "object",
    "title": "Find a dealer",
    "properties": {
        "tab_title": {
            "type": "string",
            "options": {
                "hidden": true
            },
            "template": "{{title}}",
            "watch": {
                "title": "widget.___find-a-dealer___"
            }
        },
        "___find-a-dealer___": {
            "type": "string",
